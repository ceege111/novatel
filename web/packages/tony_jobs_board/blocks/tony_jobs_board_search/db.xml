			<unsigned />
		</field>
		
		<field name="mode"  type="C" size="32"></field>  
		
		<field name="page_size"  type="i" size="4"></field> 
		
		<field name="searchableAttributes"  type="C" size="255"></field> 
		 	
		<field name="searchedCountries"  type="C" size="255"></field> 
		
		<field name="shownAttributes"  type="C" size="255"></field> 	
		
		<field name="shownLabels"  type="C" size="255"></field>
		
		<field name="hideEmptyAttrs"  type="i" size="1"></field>  
		
		<field name="showReset"  type="i" size="1"></field>   
		
		<field name="searchTitle"  type="C" size="128"></field> 
		
		<field name="searchTargetCID" type="i" size="10">
			<default value="0"/>
		</field>  
		
		<field name="cParentID" type="i" size="10">
			<default value="0"/>
		</field>  
		
		<field name="addressAsTitle"  type="i" size="1"></field> 
		
				
		<field name="displayOrder" type="C" size="64"></field>			
		
		<field name="displayOrderDesc" type="I1">
			<unsigned />
			<notnull />
			<default value="0" />
		</field>
		
		<field name="showSortBy" type="i" size="1"></field> 
		
		<field name="sortByOpts" type="C" size="255"></field>
				
	</table>
</schema>